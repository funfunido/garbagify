{
    "name": "Garbagify",
    "description": "Makes the current webpage look like garbage, by removing all CSS stylesheet links, and <style> tags.",
    "version": "1.1.5",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      

    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "service.js"
    }
}
